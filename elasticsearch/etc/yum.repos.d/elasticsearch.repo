{%- set es_version = salt['pillar.get']('elasticsearch:version', 'latest') -%}
[elasticsearch-{{ es_version }}]
name=Elasticsearch repository for {{ es_version }}.x packages
baseurl=http://packages.elastic.co/elasticsearch/{{ es_version }}/centos
gpgcheck=1
gpgkey=https://packages.elastic.co/GPG-KEY-elasticsearch
enabled=1
